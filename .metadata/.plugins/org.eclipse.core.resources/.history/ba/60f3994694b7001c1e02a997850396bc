package aulas;

public class Exemplo {

	public static void main(String[] args) {

		// Sysout + ctrl + enter
		System.out.println("Ola mundosaaddddddddddfsfsafasgsa" + "sdgdssdhdshsdhdsdshsdhdsdshhsdhsdhhsdhdhs");
		System.out.println(args[0]);
		System.out.println(args[1]);
		System.out.println(args[2]);
		int numeroDeGols = 0;
		char genero = 'X';
		
		// 00000000 - 0
		// 00000001 - 1
		// 00000010 - 2
		// 00000011 - 3
		2^8
		// logico variavel = falso
		boolean variavel = true;
		String texto = "Texto muito legal";
	}
}