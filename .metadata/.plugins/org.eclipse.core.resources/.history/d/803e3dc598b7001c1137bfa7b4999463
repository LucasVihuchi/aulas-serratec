package exercicios;

public class ExercicioVariaveis {

	public static void main(String[] args) {
		int idade = 20;
		double peso = 72.5;
		double altura = 1.83;
		String funcionario = "Lucas";
		
		System.out.println("O funcionario " + funcionario + " tem:");
		System.out.print("Idade: " + idade);
		System.out.print("Peso: " + peso);
		System.out.print("Altura: " + altura);
	}

}
